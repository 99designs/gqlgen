extend type Query {
    topProducts(first: Int = 5): [Product]
}

type Manufacturer @key(fields: "id") {
    id: String!
    name: String!
}

type Product @key(fields: "manufacturer { id } id") @key(fields: "upc") {
    id: String!
    manufacturer: Manufacturer!
    upc: String!
    name: String!
    price: Int!
}
