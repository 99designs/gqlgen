directive @goField(
    forceResolver: Boolean
    name: String
    omittable: Boolean
) on INPUT_FIELD_DEFINITION | FIELD_DEFINITION

extend type Query {
    me: User
}

type EmailHost @key(fields: "id") {
    id: String!
    name: String!
}

type User @key(fields: "id") {
    id: ID!
    host: EmailHost! @goField(forceResolver: true)
    email: String!
    username: String!
}
